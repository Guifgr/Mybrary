<h2>Search Autors</h2>
<form action="/authors" method="GET">
    <label for="name">Name</label>
    <input type="text" name="name" id="name" value="<%= searchOptions.name %>">
    <button type="submit" >Send</button>
</form>
<br>
<% authors.forEach(author =>{%>
    <div><%= author.name %> </div>
<% }) %> 